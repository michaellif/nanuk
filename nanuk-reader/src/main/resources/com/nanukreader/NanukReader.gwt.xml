<?xml version="1.0" encoding="UTF-8"?>
<!-- When updating your version of GWT, you should also update this DTD reference, so that your app can take advantage of 
				the latest GWT module capabilities. -->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
  "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='nanuk_reader'>

				<inherits name='com.google.gwt.user.User' />
				<inherits name='com.google.gwt.user.theme.clean.Clean' />
				<inherits name="com.google.gwt.xml.XML" />
				<inherits name="com.google.gwt.json.JSON" />
				<inherits name="com.google.gwt.logging.Logging" />
				<inherits name="com.google.gwt.i18n.I18N"/>
				<inherits name="com.google.gwt.junit.JUnit"/>


				<inherits name="name.pehl.totoe.xml.XML" />

				<entry-point class='com.nanukreader.client.NanukReader' />

				<source path='client' />
				<source path='shared' />

				<set-property name="gwt.logging.logLevel" value="WARNING" />
				<set-property name="gwt.logging.enabled" value="TRUE" />

</module>
